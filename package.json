{
  "name": "vue-sfcmod",
  "version": "0.6.0",
  "description": "Vue 3 SFC codemod framework",
  "type": "module",
  "bin": {
    "vue-sfcmod": "dist/bin/vue-sfcmod.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/__tests__/**",
    "sfcmod.config.example.ts"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "build:watch": "pnpm build --watch",
    "precli": "[[ -d dist/bin/ ]] || pnpm build",
    "cli": "node dist/bin/vue-sfcmod.js",
    "example": "./examples/run.sh",
    "example:clear": "git checkout examples/*/Input.*",
    "format": "pnpm prettier --check .",
    "format:staged": "pnpm prettier --write",
    "format:fix": "pnpm prettier --write .",
    "lint": "pnpm lint:eslint . && pnpm lint:types",
    "lint:fix": "pnpm lint:eslint:fix . && pnpm lint:types",
    "lint:eslint": "pnpm eslint --cache",
    "lint:eslint:staged": "pnpm lint:eslint:fix",
    "lint:eslint:fix": "pnpm lint:eslint --fix",
    "lint:types": "tsc --noEmit",
    "prepare": "husky",
    "release": "pnpm build && pnpm semantic-release",
    "playground": "npm -C ./playground run dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sidnioulz/vue-sfcmod"
  },
  "author": "Steve Dodier-Lazaro <sidnioulz@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/types": "^7.28.6",
    "@types/debug": "^4.1.12",
    "@types/inquirer": "^9.0.9",
    "@types/yargs": "^17.0.35",
    "@vue/compiler-core": "^3.5.27",
    "@vue/compiler-sfc": "^3.5.27",
    "@vue/shared": "^3.5.27",
    "cosmiconfig": "^9.0.0",
    "cosmiconfig-typescript-loader": "^6.2.0",
    "debug": "^4.4.3",
    "globby": "^16.1.0",
    "inquirer": "^13.2.1",
    "jscodeshift": "^17.3.0",
    "lint-staged": "^16.2.7",
    "lru-cache": "^11.2.5",
    "rollup-plugin-vue": "^6.0.0",
    "source-map-js": "^1.2.1",
    "vue": "^3.5.27",
    "yargs": "^18.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jscodeshift": "^17.3.0",
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/compiler-dom": "^3.5.27",
    "chalk": "^5.6.2",
    "commitizen": "^4.3.1",
    "commitlint": "^20.3.1",
    "eslint": "^9.39.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^10.7.0",
    "eslint-plugin-yml": "^3.0.0",
    "fast-check": "^4.5.3",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "memfs": "^4.56.10",
    "prettier": "^3.8.1",
    "rollup": "^4.56.0",
    "rollup-plugin-node-externals": "^8.1.2",
    "semantic-release": "^25.0.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18",
    "word-wrap": "^1.2.5",
    "zod-fast-check": "^0.10.1"
  },
  "engines": {
    "node": ">= 22.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "pnpm@10.19.0"
}
